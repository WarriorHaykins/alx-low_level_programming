#include "main.h"
/**
 *print_last_digit - prints the last digit of an integer
 *@r: the integer will last digit to be printed
 *Return: prints the last dgit of gthe integer
 */

int print_last_digit(int r)
{
int ld;
if (r >= 0)
{
ld = r % 10;
return (ld);
}
else if (r <= 0)
{
ld = -r % 10;
}
return (ld);
}
